#include<iostream>
using namespace std;

int main()
{
    int l,n,w,h;
    cin>>l;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>w>>h;
        if( w<l || h<l ) 
        {
            cout<<"UPLOAD ANOTHER"<<"\n";
            //break;
        }
        else if(w>=l && h>=l)
        {
            if(w==h) {cout<<"ACCEPTED"<<"\n";}
            else {cout<<"CROP IT"<<"\n";}
           
        }
    }
}

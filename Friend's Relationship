#include<iostream>
using namespace std;

int main()
{
	int t;
	cin>>t;
	while(t!=0)
	{
		int n=0;
		int b=0;		
		cin>>n;
		int e=(2*n)-1;
		for(int i=0;i<n;++i)
		{
			for(int j=0;j<(2*n);j++)
			{
				if(j<=b || j>=e) cout<<"*";
				else cout<<"#";
			}
			cout<<endl;
			b++;
			e--;

		}
		t--;
	}
}

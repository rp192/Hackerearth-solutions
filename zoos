#include<iostream>
using namespace std;

int main()
{
    
    int z=0;
    string str;
    cin>>str;
         if(str.length()<20)
         {
             for(int i=0;i<str.length();i++)
             {
                 if(str[i]=='z') z=z+1;
             }
			 int o=(str.length()-z);
             if((2*z)==o) cout<<"Yes"<<"\n";
         else cout<<"No"<<"\n";
         }
    return 0;
}

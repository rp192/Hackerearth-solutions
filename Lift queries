 #include<iostream>
 #include<bits/stdc++.h>
using namespace std;

int main()
{
ios_base::sync_with_stdio(false);
	 cin.tie(NULL);
	long long t;
	cin>>t;
	long long a=0;
	long long b=7;
	long long c=0;
	long long d=0;
		long long n;
	for(long long i=0;i<t;i++)
	{
	
		cin>>n;
		c=abs(a-n);
		d=abs(b-n);
		if(c<=d)
		{
			cout<<"A"<<"\n";
			a=n;
		}else
		{
			cout<<"B"<<"\n";
			b=n;
		}
		//t--;
	}

}
